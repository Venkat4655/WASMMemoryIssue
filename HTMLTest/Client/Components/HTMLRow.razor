@namespace HTMLTest.Client.Components

<tr>
    @{
        var properties = Data.GetType().GetProperties();
        @foreach (var prop in properties)
        {
		    //td rendering component
            <HTMLCell Data="@Data" PropertyInfo="@prop"></HTMLCell>           
        }
    }

</tr>

@code {

    [CascadingParameter]
    HTMLGridComponent Parent { get; set; }

    [Parameter]
    public object Data { get; set; }


    private object GetValue()
    {
        return typeof(object);
    }
}