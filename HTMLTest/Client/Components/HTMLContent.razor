@namespace HTMLTest.Client.Components

@foreach (var column in DataSource)
{
//tr rendering component
    <HTMLRow Data="@column"></HTMLRow>
  
}

@code {

    [CascadingParameter]
    HTMLGridComponent Parent { get; set; }

    [Parameter]
    public IEnumerable<object> DataSource { get; set; }

    private object GetValue()
    {
        return typeof(object);
    }
}
